<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="bit.abduhs1.dunedin">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainNavigation">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".Activities"
            android:label="@string/title_activity_activities"
            android:parentActivityName=".MainNavigation"
            android:theme="@style/AppTheme.NoActionBar">
        <meta-data
            android:name="android.support.PARENT_ACTIVITY"
            android:value="bit.abduhs1.dunedin.MainNavigation" />
        </activity>
        <activity
            android:name=".Shopping"
            android:label="@string/title_activity_shopping"
            android:parentActivityName=".MainNavigation"
            android:theme="@style/AppTheme.NoActionBar">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="bit.abduhs1.dunedin.MainNavigation" />
        </activity>
        <activity
            android:name=".Dining"
            android:label="@string/title_activity_dining"
            android:parentActivityName=".MainNavigation"
            android:theme="@style/AppTheme.NoActionBar">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="bit.abduhs1.dunedin.MainNavigation" />
        </activity>
        <activity
            android:name=".Services"
            android:label="@string/title_activity_services"
            android:parentActivityName=".MainNavigation"
            android:theme="@style/AppTheme.NoActionBar">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="bit.abduhs1.dunedin.MainNavigation" />
        </activity>
    </application>

</manifest>
