<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="bit.abduhs1.dunedin">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainNavigation">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".Activities"
            android:parentActivityName=".MainNavigation" />
        <activity android:name=".Shopping"
            android:parentActivityName=".MainNavigation" />
        <activity android:name=".Services"
            android:parentActivityName=".MainNavigation" />
        <activity android:name=".Dining"
            android:parentActivityName=".MainNavigation"></activity>
    </application>

</manifest>
